<?xml version="1.0" encoding="utf-8"?>
<SqlQuery xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Query>SQL_SIG_NAMES</Query>
  <Data><![CDATA[SELECT DISTINCT sig_name, 
                        sig_sid 
                   FROM signature 
               ORDER BY sig_name]]>
  </Data>
</SqlQuery>